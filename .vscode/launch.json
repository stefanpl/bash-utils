  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "debug testsuite",
      "protocol": "inspector",
      "preLaunchTask": "start test debugging",
      "port": 9229,
      "sourceMaps": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/home/node/app",
      "restart": true,
      "outFiles": [ "${workspaceFolder}/test/dist/tests.compiled*" ],
    },
  ]
}